  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.js",
    "dev": "nodemon src/server.ts",
    "prisma": "prisma migrate dev",
    "test:integration": "dotenv -e .env.test prisma migrate dev && NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest -- -i --testPathPattern=tests/integration",
    "test:unit": "dotenv -e .env.test prisma migrate dev && prisma db seed && NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest -- -i --coverage --testPathPattern=tests/unit",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "prettier": "npx prettier --write ."
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
